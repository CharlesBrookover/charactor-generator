{
  "name":              "landfillsoft/charactor-generator",
  "description":       "RPG Character Generator to create NPC's or pre gens for players",
  "minimum-stability": "stable",
  "license":           "MIT",
  "authors":           [
    {
      "name":  "Charles Brookover",
      "email": "charlesbrookover@gmail.com",
      "role":  "Developer"
    }
  ],
  "require":           {
    "php": "^8.1",
    "slim/slim": "4.*",
    "slim/psr7": "^1.3.0",
    "php-di/php-di": "^6.4.0",
    "php-di/slim-bridge": "^3.2.0",
    "twig/twig": "^3.0",
    "slim/twig-view": "^3.3.0",
    "symfony/yaml": "^6.1.2"
  },
  "require-dev":       {
    "phpunit/phpunit":  "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Landfill\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Landfill\\Tests\\": "tests/"
    }
  }
}
